<!doctype html>
<html lang="en">

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport"
        content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1, viewport-fit=cover" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
    <meta name="theme-color" content="#000000">
    <title>Profile - <%=user.fullname%></title>
    <meta name="description" content="zoe1">
    <meta name="keywords"
        content="bootstrap, wallet, banking, fintech mobile template, cordova, phonegap, mobile, html, responsive" />
    <link rel="icon" type="image/png" href="assets/img/favicon.png" sizes="32x32">
    <link rel="apple-touch-icon" sizes="180x180" href="assets/img/icon/192x192.png">
    <link rel="stylesheet" href="assets/css/style.css">
    <link rel="manifest" href="__manifest.json">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    <script src="../plugins/sweetalerts/promise-polyfill.js"></script>
    <link href="../plugins/sweetalerts/sweetalert.css" rel="stylesheet" type="text/css" />
</head>

<body style="background-color:#000000;">

    <!-- App Header -->
    <div class="appHeader" style="background-color:#191919;">
        <div class="left">
            <a href="/dashboard" class="headerButton goBack">
                <i class="bi bi-chevron-left" style="color:#f2f2f2;"></i>
            </a>
        </div>
        <div class="pageTitle" style="color:#f2f2f2;">
            About
        </div>
        <div class="right">
        </div>
    </div>
    <!-- * App Header -->

    <!-- App Capsule -->
    <div id="appCapsule">
        <div class="section mt-3 text-center">
            <div class="avatar-section">
                <a href="#">
                    <img src="assets/img/sample/avatar/avatar1.jpg" alt="avatar" class="imaged w100 rounded">
                </a>
            </div>
        </div>
        <div class="section mt-3 mb-3">
            <div class="card" style="background-color:#191919; color:#cecece;">
                <div class="card-body">
                    <h2 class="card-title" style="color:#f2f2f2;">Fullname</h2>
                    <%=user.firstname%>    <%=user.lastname%>
                </div>
            </div>
        </div>
        <div class="section mt-3 mb-3">
            <div class="card" style="background-color:#191919; color:#cecece;">
                <div class="card-body">
                    <h2 class="card-title" style="color:#f2f2f2;">Contact</h2>
                    <a href="callto:Instagram"><%=user.tel%></a>
                </div>
            </div>
        </div>
        <div class="section mt-3 mb-3">
            <div class="card" style="background-color:#191919; color:#cecece;">
                <div class="card-body">
                    <h2 class="card-title" style="color:#f2f2f2;">Email</h2>
                    <a href="mailto:cshrem04@gmail.com"><%=user.email%></a>
                </div>
            </div>
        </div>
        <div class="section mt-3 mb-3">
            <div class="card" style="background-color:#191919; color:#cecece;">
                <div class="card-body">
                    <h2 class="card-title" style="color:#f2f2f2;">Country</h2>
                    <%=user.country%>
                </div>
            </div>
        </div>
        <div class="section mt-3 mb-3">
            <div class="card" style="background-color:#191919; color:#cecece;">
                <div class="card-body">
                    <h2 class="card-title" style="color:#f2f2f2;">Currency</h2>
                    <%=user.currency%>
                </div>
            </div>
        </div>
        <div class="section mt-3 mb-3">
            <div class="card" style="background-color:#191919; color:#cecece;">
                <div class="card-body">
                    <a href="/logout" class="card-title" style="color:#f2f2f2;">Logout</a>
                </div>
            </div>
        </div>
    </div>
    <!-- * App Capsule -->

    <!-- App Bottom Menu -->
    <div class="appBottomMenu">
        <a href="/dashboard" class="item">
            <div class="col">
                <i class="bi bi-pie-chart"></i>
                <strong>Dashboard</strong>
            </div>
        </a>
        <a href="/deposit" class="item">
            <div class="col">
                <i class="bi bi-cash"></i>
                <strong>Deposit</strong>
            </div>
        </a>
        <a href="/widthdrawHistory/<%=user._id%>" class="item">
            <div class="col">
                <i class="bi bi-grid"></i>
                <strong>Transaction</strong>
            </div>
        </a>
        <a href="/withdraw" class="item">
            <div class="col">
                <i class="bi bi-wallet2"></i>
                <strong>Withdrawal</strong>
            </div>
        </a>
        <a href="/profile" class="item">
            <div class="col">
                <i class="bi bi-person"></i>
                <strong>Profile</strong>
            </div>
        </a>
    </div>
    <!-- * App Bottom Menu -->

    <!-- App Sidebar -->
    <div class="modal fade panelbox panelbox-left" id="sidebarPanel" tabindex="-1" role="dialog">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-body p-0">
                    <!-- profile box -->
                    <div class="profileBox pt-2 pb-2">
                        <div class="image-wrapper">
                            <img src="assets/img/sample/avatar/avatar1.jpg" alt="image" class="imaged w36">
                        </div>
                        <div class="in">
                            <strong><%=user.fullname%></strong>
                            <div class="text-muted"> Account Plan: <%=user.asset%></div>
                        </div>
                        <a href="#" class="btn btn-link btn-icon sidebar-close" data-bs-dismiss="modal">
                            <i class="bi bi-x"></i>
                        </a>
                    </div>
                    <!-- * profile box -->
                    <!-- balance -->
                    <div class="sidebar-balance">
                        <div class="listview-title">Balance</div>
                        <div class="in">
                            <h1 class="amount"><%=user.balance%></h1>
                        </div>
                    </div>
                    <!-- * balance -->

                    <!-- action group -->
                    <div class="action-group">
                        <a href="/deposit" class="action-button">
                            <div class="in">
                                <div class="iconbox">
                                    <i class="bi bi-plus"></i>
                                </div>
                                Deposit
                            </div>
                        </a>
                        <a href="/withdraw" class="action-button">
                            <div class="in">
                                <div class="iconbox">
                                    <i class="bi bi-wallet2"></i>
                                </div>
                                Withdraw
                            </div>
                        </a>
                        <a href="/widthdrawHistory/<%=user._id%>" class="action-button">
                            <div class="in">
                                <div class="iconbox">
                                    <i class="bi bi-credit-card"></i>
                                </div>
                                History
                            </div>
                        </a>
                        <a href="/depositHistory/<%=user._id%>" class="action-button">
                            <div class="in">
                                <div class="iconbox">
                                    <i class="bi bi-arrow-down-up"></i>
                                </div>
                                Transaction
                            </div>
                        </a>
                    </div>
                    <!-- * action group -->

                    <!-- menu -->
                    <div class="listview-title mt-1">Menu</div>
                    <ul class="listview flush transparent no-line image-listview">
                        <li>
                            <a href="/verify" class="item">
                                <div class="icon-box bg-primary">
                                    <i class="bi bi-file-text"></i>
                                </div>
                                <div class="in">
                                    Verify Your Account (KYC)
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="/dashboard" class="item">
                                <div class="icon-box bg-primary">
                                    <i class="bi bi-pie-chart"></i>
                                </div>
                                <div class="in">
                                    Dashboard
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="/deposit" class="item">
                                <div class="icon-box bg-primary">
                                    <i class="bi bi-cash"></i>
                                </div>
                                <div class="in">
                                    Deposit
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="/withdraw" class="item">
                                <div class="icon-box bg-primary">
                                    <i class="bi bi-wallet2"></i>
                                </div>
                                <div class="in">
                                    Withdrawal
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="/insurance" class="item">
                                <div class="icon-box bg-primary">
                                    <i class="bi bi-shield-check"></i>
                                </div>
                                <div class="in">
                                    Insurance Deposit
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="/signal" class="item">
                                <div class="icon-box bg-primary">
                                    <i class="bi bi-lightning"></i>
                                </div>
                                <div class="in">
                                    Signal Activation
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="/upgrade" class="item">
                                <div class="icon-box bg-primary">
                                    <i class="bi bi-graph-up"></i>
                                </div>
                                <div class="in">
                                    Upgrade
                                </div>
                            </a>
                        </li>
                    </ul>
                    <!-- * menu -->

                    <!-- others -->
                    <div class="listview-title mt-1">Others</div>
                    <ul class="listview flush transparent no-line image-listview">
                        <li>
                            <a href="/widthdrawHistory/<%=user._id%>" class="item">
                                <div class="icon-box bg-primary">
                                    <i class="bi bi-arrow-repeat"></i>
                                </div>
                                <div class="in">
                                    Earning History
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="/depositHistory/<%=user._id%>" class="item">
                                <div class="icon-box bg-primary">
                                    <i class="bi bi-qr-code"></i>
                                </div>
                                <div class="in">
                                    Transactions
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="/referral-history" class="item">
                                <div class="icon-box bg-primary">
                                    <i class="bi bi-link-45deg"></i>
                                </div>
                                <div class="in">
                                    Referral History
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="/profile" class="item">
                                <div class="icon-box bg-primary">
                                    <i class="bi bi-person"></i>
                                </div>
                                <div class="in">
                                    Profile
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="/logout" class="item">
                                <div class="icon-box bg-primary">
                                    <i class="bi bi-box-arrow-right"></i>
                                </div>
                                <div class="in">
                                    Log out
                                </div>
                            </a>
                        </li>
                    </ul>
                    <!-- * others -->
                </div>
            </div>
        </div>
    </div>
    <!-- * App Sidebar -->

    <!-- iOS Add to Home Action Sheet -->
    <div class="modal inset fade action-sheet ios-add-to-home" id="ios-add-to-home-screen" tabindex="-1" role="dialog">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Add to Home Screen</h5>
                    <a href="#" class="close-button" data-bs-dismiss="modal">
                        <i class="bi bi-x"></i>
                    </a>
                </div>
                <div class="modal-body">
                    <div class="action-sheet-content text-center">
                        <div class="mb-1"><img src="assets/img/icon/192x192.png" alt="image" class="imaged w64 mb-2"></div>
                        <div>Install <strong>Finapp</strong> on your iPhone's home screen.</div>
                        <div>Tap <i class="bi bi-share"></i> and Add to homescreen.</div>
                        <div class="mt-2">
                            <button class="btn btn-primary btn-block" data-bs-dismiss="modal">CLOSE</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- * iOS Add to Home Action Sheet -->

    <!-- Android Add to Home Action Sheet -->
    <div class="modal inset fade action-sheet android-add-to-home" id="android-add-to-home-screen" tabindex="-1" role="dialog">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Add to Home Screen</h5>
                    <a href="#" class="close-button" data-bs-dismiss="modal">
                        <i class="bi bi-x"></i>
                    </a>
                </div>
                <div class="modal-body">
                    <div class="action-sheet-content text-center">
                        <div class="mb-1">
                            <img src="assets/img/icon/192x192.png" alt="image" class="imaged w64 mb-2">
                        </div>
                        <div>Install <strong>Finapp</strong> on your Android's home screen.</div>
                        <div>Tap <i class="bi bi-three-dots-vertical"></i> and Add to homescreen.</div>
                        <div class="mt-2">
                            <button class="btn btn-primary btn-block" data-bs-dismiss="modal">CLOSE</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- * Android Add to Home Action Sheet -->

    <div id="cookiesbox" class="offcanvas offcanvas-bottom cookies-box" tabindex="-1" data-bs-scroll="true" data-bs-backdrop="false">
        <div class="offcanvas-header">
            <h5 class="offcanvas-title">We use cookies</h5>
        </div>
        <div class="offcanvas-body">
            <div>
                Lorem ipsum dolor sit amet, consectetur adipiscing elit. Nulla non lacinia quam. Nulla facilisi.
                <a href="#" class="text-secondary"><u>Learn more</u></a>
            </div>
            <div class="buttons">
                <a href="#" class="btn btn-primary btn-block" data-bs-dismiss="offcanvas">I understand</a>
            </div>
        </div>
    </div>

    <!-- ========= JS Files =========  -->
    <!-- Bootstrap -->
    <script src="../plugins/jquery-3.1.1.min.js"></script>
    <script src="../plugins/sweetalerts/sweetalert2.min.js"></script>
    <script src="https://unpkg.com/@popperjs/core@2/dist/umd/popper.js"></script>
    <script src="assets/js/lib/bootstrap.bundle.min.js"></script>
    <!-- Ionicons (Removed since we're using Bootstrap Icons) -->
    <!-- <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script> -->
    <!-- Splide -->
    <script src="assets/js/plugins/splide/splide.min.js"></script>
    <!-- Base Js File 
    <script src="assets/js/base.js"></script>-->

    <script>
        // Add to Home with 2 seconds delay.
        AddtoHome("2000", "once");
    </script>

</body>

</html>